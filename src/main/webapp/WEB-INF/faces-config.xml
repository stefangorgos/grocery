<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">

	<navigation-rule>
		<from-view-id>/comment.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>comments</from-outcome>
			<to-view-id>/comments.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/order.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>orders</from-outcome>
			<to-view-id>/order.jsf</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
   
</faces-config>
